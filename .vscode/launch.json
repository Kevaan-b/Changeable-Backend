{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/Changeable-Backend",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/Changeable-Backend"
            }

        }
    ]
}
